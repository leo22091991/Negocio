<p id="notice"><%= notice %></p>

<h1>Salaries</h1>

<table>
  <thead>
    <tr>
      <th>dni</th>
      <th>Apellido</th>
      <th>Nombres</th>
      <th>Fecha</th>
      <th>estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @salaries.each do |salary| %>
      <tr>
        <td><%= salary.user.dni %></td>
        <td><%= salary.user.last_name %></td>
        <td><%= salary.user.name %></td>
        <td><%= salary.updated_at %></td>
        <td></td>
        <td><%= link_to 'Show', salary %></td>
        <td><%= link_to 'Edit', edit_salary_path(salary) %></td>
        <td><%= link_to 'Destroy', salary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Salary', new_salary_path %>
